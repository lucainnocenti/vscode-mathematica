{
    "name": "mathematica",
    "displayName": "mathematica",
    "description": "Language support for Wolfram Mathematica",
    "version": "0.0.1",
    "publisher": "myself",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "mathematica",
            "aliases": ["Wolfram Mathematica", "mathematica"],
            "extensions": [".m"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mathematica",
            "scopeName": "source.mathematica",
            "path": "./syntaxes/mathematica.tmLanguage.json"
        }]
    }
}